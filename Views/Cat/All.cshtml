@model List<CatViewModel>
<h1> Cat Information</h1>
<hr />

<table>
    <th>
    <td>Name</td>
    <td>Age</td>
    <td>Bread</td>
    <td>Image</td>
    </th>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Breed</td>
            <td><img src="@item.ImgUrl" width="20%" /></td>


            <td> <a class="nav-link text-dark" asp-area="" asp-controller="Cat" asp-action="Details" asp-route-id="@item.Id">Details</a> </td>
            <td> <a class="nav-link text-dark" asp-area="" asp-controller="Cat" asp-action="Edit" asp-route-id="@item.Id">Edit</a> </td>
            <td> <a class="nav-link text-dark" asp-area="" asp-controller="Cat" asp-action="Delete" asp-route-id="@item.Id">Delete</a> </td>
        </tr>
    }


</table>